<script src="~/js/guage/gauge.min.js"></script>
<script>
    var gauge = new RadialGauge({
        renderTo: 'guage',
        width: 350,
        height: 350,
        units: "People/Hr",
        minValue: 0,
        maxValue: 1000,
        majorTicks: [
            "0",
            "100",
            "200",
            "300",
            "400",
            "500",
            "600",
            "700",
            "800",
            "900",
            "1000"
        ],
        minorTicks: 5,
        strokeTicks: true,
        highlights: [
            {
                "from": 900,
                "to": 1000,
                "color": "rgba(200, 50, 50, .75)"
            }
        ],
        colorPlate: "#fff",
        borderShadowWidth: 0,
        borders: true,
        needleType: "arrow",
        needleWidth: 2,
        needleCircleSize: 7,
        needleCircleOuter: true,
        needleCircleInner: false,
        animationDuration: 1500,
        animationRule: "linear"
    }).draw();

    setInterval(() => {
        gauge.value = Math.random() * 500 + 200
    }, 2000);
</script>
